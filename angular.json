{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "annuaire_angular": {
      "projectType": "application",
      "schematics": {},
      "root": "",
      "sourceRoot": "src",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "@angular/build:application",
          "options": {
            "browser": "src/main.ts",
            "tsConfig": "tsconfig.app.json",
            "assets": [
              {
                "glob": "**/*",
                "input": "public"
              },
              {
                "glob": "**/*",
                "input": "src/assets",
                "output": "/assets"
              }

            ],
            "scripts": [
              "src/assets/plugins/bootstrap/js/bootstrap.bundle.min.js",
              "src/assets/js/jquery-3.6.0.min.js",
              "src/assets/js/feather.min.js",
              "src/assets/js/script.js"
            ],
            "styles": [
              "node_modules/@ng-select/ng-select/themes/default.theme.css",
              "src/styles.css",
              "src/assets/css/styles.scss",
              "node_modules/bootstrap/dist/css/bootstrap.min.css",
              "src/assets/css/style.css",
              "src/assets/plugins/bootstrap/css/bootstrap.min.css",
              "src/assets/plugins/feather/feather.css",
              "src/assets/plugins/fontawesome/css/fontawesome.min.css",
              "src/assets/plugins/fontawesome/css/all.min.css",
              "src/styles.css",
              "node_modules/leaflet/dist/leaflet.css"
            ],
            "polyfills": [
              "@angular/localize/init"
            ]
          },
          "configurations": {
            "production": {
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "500kB",
                  "maximumError": "1MB"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "4kB",
                  "maximumError": "8kB"
                }
              ],
              "outputHashing": "all"
            },
            "development": {
              "optimization": false,
              "extractLicenses": false,
              "sourceMap": true
            }
          },
          "defaultConfiguration": "production"
        },
        "serve": {
          "builder": "@angular/build:dev-server",
          "configurations": {
            "production": {
              "buildTarget": "annuaire_angular:build:production"
            },
            "development": {
              "buildTarget": "annuaire_angular:build:development"
            }
          },
          "defaultConfiguration": "development"
        },
        "extract-i18n": {
          "builder": "@angular/build:extract-i18n"
        },
        "test": {
          "builder": "@angular/build:karma",
          "options": {
            "tsConfig": "tsconfig.spec.json",
            "assets": [
              {
                "glob": "**/*",
                "input": "public"
              },
              {
                "glob": "**/*",
                "input": "src/assets",
                "output": "/assets"
              }
            ],
            "scripts": [
              "node_modules/bootstrap/dist/js/bootstrap.bundle.min.js",
              "assets/js/jquery-3.6.0.min.js",
              "assets/plugins/bootstrap/js/bootstrap.bundle.min.js",
              "assets/js/feather.min.js",
              "assets/js/script.js"
            ],
            "styles": [
              "node_modules/bootstrap/dist/css/bootstrap.min.css",
              "src/assets/css/style.css",
              "src/assets/plugins/bootstrap/css/bootstrap.min.css",
              "src/assets/plugins/feather/feather.css",
              "src/assets/plugins/icons/flags/flags.css",
              "src/assets/plugins/fontawesome/css/fontawesome.min.css",
              "src/assets/plugins/fontawesome/css/all.min.css",
              "src/styles.css"
            ],
            "polyfills": [
              "@angular/localize/init"
            ]
          }
        }
      }
    }
  },
  "cli": {
    "analytics": "31130bd0-8d14-43db-a750-dd96a81a0d25"
  }
}
